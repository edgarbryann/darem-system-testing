  <script type="text/javascript">
    var map = L.map('map').setView([14.100072, 122.6615868], 10);

    var tiles = L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
      maxZoom: 18,
      attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors, Imagery 漏 <a href="https://www.mapbox.com/">Mapbox</a>',
    }).addTo(map);

    // get color depending on population density value
    function getColor(d) {
      return d == "Sta Elena" ? '#5584AC' :
        d == "Daet" ? '#333C83' :
          d == "Labo" ? '#F24A72' :
            d == "Capalonga" ? '#95D1CC' :
              d == "Basud" ? '#EAEA7F' :
                d == "Jose Panganiban" ? '#A97155' :
                  d == "Mercedes" ? '#FFA8A8' :
                    d == "San Lorenzo" ? '#F7F5F2' :
                      d == "Paracale" ? '#712B75' :
                        d == "San Vicente" ? '#D49B54' :
                          d == "Talisay" ? '#712B75' :
                            d == "Vinzonz" ? '#FED976' : '#FFEDA0';
    }

    function style(feature) {
      return {
        weight: 2,
        opacity: 1,
        color: 'white',
        dashArray: '3',
        fillOpacity: 0.7,
        fillColor: getColor(feature.properties.ADM3_EN)
      };
    }
  map.invalidateSize(); 
